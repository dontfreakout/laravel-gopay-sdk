{
    "name": "hazestudio/laravel-gopay-sdk",
    "description": "GoPay SDK integration with Laravel Framework",
    "type": "library",
    "require": {
        "php": "^7.2",
        "illuminate/support": ">=5.0",
        "illuminate/config": ">=5.0",
        "illuminate/cache": ">=5.0",
        "illuminate/container": ">=5.0",
        "gopay/payments-sdk-php": "^1.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "HazeStudio\\LaravelGoPaySDK\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Haze Studio",
            "email": "studio@haze.sk"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "HazeStudio\\LaravelGoPaySDK\\GopayServiceProvider"
            ],
            "aliases": {
                "Debugbar": "HazeStudio\\LaravelGoPaySDK\\Facade"
            }
        }
    }
}
